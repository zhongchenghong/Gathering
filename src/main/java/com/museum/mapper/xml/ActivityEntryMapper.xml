                    <result column="fileaddress" property="fileaddress"/>
                    <result column="createtimes" property="createtimes"/>
                    <result column="state" property="state"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, uid, starttime, endtime, name, release, activity_address, people_sum, filename, fileaddress, createtimes,state
        </sql>

</mapper>