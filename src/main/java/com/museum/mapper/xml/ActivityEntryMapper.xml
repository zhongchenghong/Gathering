<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.ActivityEntryMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.ActivityEntry">
                    <id column="id" property="id"/>
                    <result column="uid" property="uid"/>
                    <result column="starttime" property="starttime"/>
                    <result column="endtime" property="endtime"/>
                    <result column="name" property="name"/>
                    <result column="release" property="release"/>
                    <result column="activity_address" property="activityAddress"/>
