<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.CatalogueMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.Catalogue">
                    <id column="id" property="id"/>
                    <result column="parentId" property="parentId"/>
                    <result column="name" property="name"/>
                    <result column="type" property="type"/>
                    <result column="catalogue_type" property="catalogueType"/>
                    <result column="createtimes" property="createtimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, parentId, name, type, catalogue_type, createtimes
        </sql>

</mapper>