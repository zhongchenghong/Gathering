<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.ChargeMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.Charge">
                    <id column="id" property="id"/>
                    <result column="load" property="load"/>
                    <result column="region" property="region"/>
                    <result column="num" property="num"/>
                    <result column="total" property="total"/>
                    <result column="createtimes" property="createtimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, load, region, num, total, createtimes
        </sql>

</mapper>