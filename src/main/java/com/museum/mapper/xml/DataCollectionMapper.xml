                    <result column="shotcontent" property="shotcontent"/>
                    <result column="pdepartment" property="pdepartment"/>
                    <result column="sum" property="sum"/>
                    <result column="complete" property="complete"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, uid, applytime, applydepartment, shottime, shotname, shottype, shotcontent, pdepartment, sum, complete
        </sql>

</mapper>