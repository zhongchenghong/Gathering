<mapper namespace="com.museum.dao.DataCollectionMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.DataCollection">
                    <id column="id" property="id"/>
                    <result column="uid" property="uid"/>
                    <result column="applytime" property="applytime"/>
                    <result column="applydepartment" property="applydepartment"/>
                    <result column="shottime" property="shottime"/>
                    <result column="shotname" property="shotname"/>
