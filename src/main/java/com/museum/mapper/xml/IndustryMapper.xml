                    <result column="usage" property="usage"/>
                    <result column="situation" property="situation"/>
                    <result column="escrow_agent" property="escrowAgent"/>
                    <result column="term" property="term"/>
                    <result column="payment_period" property="paymentPeriod"/>
                    <result column="lease_term" property="leaseTerm"/>
                    <result column="rent" property="rent"/>
                    <result column="remarks" property="remarks"/>
                    <result column="uid" property="uid"/>
                    <result column="createtimes" property="createtimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, name, address, card, area, value, usage, situation, escrow_agent, term, payment_period, lease_term, rent, remarks, uid, createtimes
        </sql>

</mapper>