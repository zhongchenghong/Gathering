<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.NotStructureMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.NotStructure">
                    <id column="id" property="id"/>
                    <result column="name" property="name"/>
                    <result column="depment" property="depment"/>
                    <result column="user_name" property="userName"/>
                    <result column="filename" property="filename"/>
                    <result column="fileaddress" property="fileaddress"/>
                    <result column="uid" property="uid"/>
                    <result column="createtimes" property="createtimes"/>
                    <result column="type" property="type"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, name, depment, user_name, filename, fileaddress, uid, createtimes, type
        </sql>

</mapper>