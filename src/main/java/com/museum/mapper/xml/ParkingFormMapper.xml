<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.ParkingFormMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.ParkingForm">
                    <id column="id" property="id"/>
                    <result column="uid" property="uid"/>
                    <result column="parking_name" property="parkingName"/>
                    <result column="createtime" property="createtime"/>
                    <result column="parkingtime" property="parkingtime"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, uid, parking_name, createtime, parkingtime
        </sql>

</mapper>