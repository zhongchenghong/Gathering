<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.ProductIncomeMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.ProductIncome">
                    <id column="id" property="id"/>
                    <result column="supplier_name" property="supplierName"/>
                    <result column="product_name" property="productName"/>
                    <result column="sale_sum" property="saleSum"/>
                    <result column="sale_discount" property="saleDiscount"/>
                    <result column="money" property="money"/>
                    <result column="type" property="type"/>
                    <result column="createtimes" property="createtimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, supplier_name, product_name, sale_sum, sale_discount, money, type, createtimes
        </sql>

</mapper>