<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.ReceiptMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.Receipt">
                    <id column="id" property="id"/>
                    <result column="uid" property="uid"/>
                    <result column="creatimes" property="creatimes"/>
                    <result column="department" property="department"/>
                    <result column="times" property="times"/>
                    <result column="titles" property="titles"/>
                    <result column="num" property="num"/>
                    <result column="idea" property="idea"/>
                    <result column="undertake_department" property="undertakeDepartment"/>
                    <result column="fen_director_time" property="fenDirectorTime"/>
                    <result column="fen_director_state" property="fenDirectorState"/>


        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, uid, creatimes, department, times, titles, num, idea, undertake_department,fen_director_state,fen_director_time
        </sql>

</mapper>