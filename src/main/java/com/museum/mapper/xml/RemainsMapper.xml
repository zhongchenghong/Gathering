<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.RemainsMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.Remains">
                    <id column="id" property="id"/>
                    <result column="num" property="num"/>
                    <result column="address" property="address"/>
                    <result column="content" property="content"/>
                    <result column="line" property="line"/>
                    <result column="filename" property="filename"/>
                    <result column="fileaddress" property="fileaddress"/>
                    <result column="uid" property="uid"/>
                    <result column="createtimes" property="createtimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, num, address, content, line, filename, fileaddress, uid, createtimes
        </sql>

</mapper>