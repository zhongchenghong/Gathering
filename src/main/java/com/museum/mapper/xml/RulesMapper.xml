<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.RulesMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.Rules">
                    <id column="id" property="id"/>
                    <result column="rules_context" property="rulesContext"/>
                    <result column="rules_title" property="rulesTitle"/>
                    <result column="uid" property="uid"/>
                    <result column="rules_filename" property="rulesFilename"/>
                    <result column="rules_address" property="rulesAddress"/>
                    <result column="createTimes" property="createTimes"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, rules_context, rules_title, uid, rules_filename, rules_address, createTimes
        </sql>

</mapper>