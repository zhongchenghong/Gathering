<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.museum.dao.UserMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.museum.domain.User">
                    <id column="id" property="id"/>
                    <result column="account" property="account"/>
                    <result column="password" property="password"/>
                    <result column="creatdate" property="creatdate"/>
                    <result column="authentication_id" property="authenticationId"/>
                    <result column="phone" property="phone"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, account, password,creatdate, authentication_id, phone
        </sql>

</mapper>